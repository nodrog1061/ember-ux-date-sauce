{{#uxs-form-control-row name="range-day"}}
  {{#uxs-form-control-row name="range-day-start" as |row|}}
    {{row.icon 'date_range' prefix=true}}
    {{uxs-form-date
      value=(readonly value)
      onChange=(action "setDate")
      placeholder="Date"
      enableTime=false
    }}
  {{/uxs-form-control-row}}
{{/uxs-form-control-row}}